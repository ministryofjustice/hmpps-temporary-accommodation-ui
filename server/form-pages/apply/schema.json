{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "type": "object",
  "title": "Apply Schema",
  "additionalProperties": false,
  "required": [
    "example-task",
    "check-your-answers"
  ],
  "properties": {
    "example-task": {
      "type": "object",
      "properties": {
        "example-page": {
          "type": "object",
          "properties": {
            "exampleAnswer": {
              "enum": [
                "no",
                "yes"
              ],
              "type": "string"
            }
          }
        }
      }
    },
    "check-your-answers": {
      "type": "object",
      "properties": {
        "review": {
          "type": "object",
          "properties": {
            "reviewed": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}